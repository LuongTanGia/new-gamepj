{
	"cSpell.words": [
    "elif",
    "hitbox",
    "knockback",
    "onready",
    "spikey",
  ],
	"prettier.enable": false,
	"editor.formatOnSave": true,
	"files.exclude": {
		"**/.uid": true,
		"**/.import": false,
	},
	"editor.detectIndentation": true,
	"editor.tabSize": 2,
  "editor.useTabStops": true,
	"editor.rulers": [
		80,
		120
	],
  "files.eol": "\n"
}
